    <hr />

    <section class="container">
        <div class="fade-in button body">
            <img src="" alt="" />
            <a asp-area="" asp-controller="CAD" asp-action="Add">Order Your Design</a>
        </div>
        <div class="fade-in button body">
            <img src="" alt="" />
            <a asp-area="" asp-controller="CAD" asp-action="Categories">Choose From Gallery</a>
        </div>
